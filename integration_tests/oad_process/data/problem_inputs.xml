<FASTOAD_model>
  <data>
    <TLAR>
      <v_cruise units="kn" is_input="True">160.0</v_cruise>
    </TLAR>
    <geometry>
      <has_T_tail is_input="True">0.0<!--0=horizontal tail is attached to fuselage / 1=horizontal tail is attached to top of vertical tail--></has_T_tail>
      <cabin>
        <aisle_width units="m" is_input="True">0.0<!--width of aisles--></aisle_width>
        <luggage>
          <mass_max units="kg" is_input="True">60.0</mass_max>
        </luggage>
        <seats>
          <passenger>
            <NPAX_max is_input="True">2.0</NPAX_max>
            <count_by_row is_input="True">3.0</count_by_row>
            <length units="m" is_input="True">0.8</length>
            <width units="m" is_input="True">0.4</width>
          </passenger>
          <pilot>
            <length units="m" is_input="True">0.75</length>
            <width units="m" is_input="True">0.565</width>
          </pilot>
        </seats>
      </cabin>
      <horizontal_tail>
        <area units="m**2" is_input="True">2.75<!--horizontal tail area--></area>
        <sweep_25 units="deg" is_input="True">4.0<!--sweep angle at 25% chord of horizontal tail--></sweep_25>
        <taper_ratio is_input="True">0.613<!--taper ratio of horizontal tail--></taper_ratio>
        <MAC>
          <at25percent>
            <x>
              <from_wingMAC25 units="m" is_input="True">3.91<!--distance along X between 25% MAC of wing and 25% MAC of horizontal tail--></from_wingMAC25>
            </x>
          </at25percent>
        </MAC>
      </horizontal_tail>
      <propulsion>
        <count is_input="True">1.0</count>
        <layout is_input="True">3.0<!--position of engines (1=under the wing / 2=rear fuselage)--></layout>
        <y_ratio is_input="True">0.34</y_ratio>
      </propulsion>
      <vertical_tail>
        <area units="m**2" is_input="True">1.51<!--vertical tail area--></area>
        <aspect_ratio is_input="True">2.0<!--aspect ratio of vertical tail--></aspect_ratio>
        <sweep_25 units="deg" is_input="True">10.0<!--sweep angle at 25% chord of vertical tail--></sweep_25>
        <taper_ratio is_input="True">0.502<!--taper ratio of vertical tail--></taper_ratio>
      </vertical_tail>
      <wing>
        <area units="m**2" is_input="True">13.46<!--wing reference area--></area>
        <aspect_ratio is_input="True">10.12<!--wing aspect ratio--></aspect_ratio>
        <sweep_25 units="deg" is_input="True">0.0<!--sweep angle at 25% chord of wing--></sweep_25>
        <taper_ratio is_input="True">0.5<!--taper ratio of wing--></taper_ratio>
        <thickness_ratio is_input="True">0.12<!--mean thickness ratio of wing--></thickness_ratio>
        <kink>
          <span_ratio is_input="True">0.0<!--ratio (Y-position of kink)/(semi-span)--></span_ratio>
        </kink>
        <MAC>
          <at25percent>
            <x units="m" is_input="True">2.65<!--X-position of the 25% of mean aerodynamic chord of wing w.r.t. aircraft nose (drives position of wing along fuselage)--></x>
          </at25percent>
        </MAC>
      </wing>
    </geometry>
    <propulsion>
      <IC_engine>
        <fuel_type is_input="True">1.0</fuel_type>
        <max_power units="W" is_input="True">231000.0</max_power>
        <strokes_nb is_input="True">4.0</strokes_nb>
      </IC_engine>
    </propulsion>
    <mission>
      <sizing>
        <main_route>
          <cruise>
            <altitude units="ft" is_input="True">8000.0<!--altitude during cruise phase in sizing mission--></altitude>
          </cruise>
        </main_route>
      </sizing>
    </mission>
  </data>
</FASTOAD_model>
